#include <stdio.h>
#include <stdlib.h>
#define importantvalue 0

int int_sort2(int *p1, int *p2);
int int_sort3(int *p1, int *p2, int *p3);

int
main(int argc, char *argv[]) {
	int x, y;
	printf("Enter some stuff: ");
	scanf("%d %d", &x, &y);
	int_sort2(&x,&y);
	printf("%d is less than or equal to %d\n", x, y);
	return 0;
}


int 
int_sort2(int *p1, int *p2){
	int temp=0;
	if (*p1>*p2){
		temp=*p1;
		*p1=*p2;
		*p2=temp;
	}
	return 0;
}
		

int 
int_sort3(int *p1, int *p2, int *p3){
	int_sort2(*p1, *p2);
	int_sort2(*p2, *p3);
	int_sort2(*p1, *p2);
	return 0;
}
	
